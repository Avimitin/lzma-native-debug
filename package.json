	],
	"homepage": "https://github.com/addaleax/lzma-native",
	"licenses": [
		{
			"type": "LGPL-3.0+",
			"url": "https://github.com/addaleax/lzma-native/raw/master/LICENSE"
		}
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/addaleax/lzma-native.git"
	},
	"scripts": {
		"install": "node-gyp rebuild",
		"test": "mocha -s 200 test"
	},
	"gypfile": true,
	"bugs": {
		"url": "https://github.com/addaleax/lzma-native/issues"
	},
	"devDependencies": {
		"mocha": "*",
